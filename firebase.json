{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/Game",
      "destination": "/index.html"
    }, 
    {
      "source": "/About",
      "destination": "/index.html"
    },
    {
      "source": "/Home",
      "destination": "/index.html"
    } ]
  }
}
